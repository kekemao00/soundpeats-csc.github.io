    server: vps-jp.sounxx1.com
    port: 443
    password: adsas
    skip-cert-verify: true
    udp: true
  
  - name: "🇺🇸美国-Trojan"
    type: trojan
    server: us-vps.kekemao.me
    port: 443
    password: kekemao
